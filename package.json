{
	"name": "@gisatcz/ptr-timeline",
	"version": "0.0.4",
	"description": "Panther timeline React component",
	"keywords": [
		"panther",
		"gisat",
		"timeline",
		"component"
	],
	"homepage": "https://github.com/gisat-panther/ptr-timeline",
	"prettier": "@gisatcz/prettier-config",
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"style": "dist/style.css",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"files": [
		"dist",
		"lib"
	],
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/gisat-panther/ptr-timeline"
	},
	"dependencies": {
		"@gisatcz/ptr-core": "^1.3.11",
		"@gisatcz/ptr-utils": "^1.3.11",
		"classnames": "2.2.6",
		"lodash": "^4.17.21",
		"moment": "2.29.1",
		"node-sass": "^4.14.1",
		"path": "0.12.7",
		"postcss": "^8.3.5",
		"prop-types": "15.7.2",
		"react": "^16.13.1",
		"react-resize-detector": "^5.0.6",
		"sass-loader": "^8.0.2",
		"source-map-loader": "^1.0.1",
		"style-loader": "^1.2.1",
		"svg-url-loader": "^6.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.12.16",
		"@babel/core": "^7.12.16",
		"@babel/plugin-proposal-class-properties": "^7.12.13",
		"@babel/preset-env": "^7.12.16",
		"@babel/preset-react": "^7.12.13",
		"@babel/register": "^7.12.13",
		"@gisatcz/prettier-config": "0.0.3",
		"@rollup/plugin-alias": "^3.1.2",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^17.1.0",
		"@rollup/plugin-json": "4.1.0",
		"@rollup/plugin-multi-entry": "3.0.1",
		"@rollup/plugin-node-resolve": "^11.1.1",
		"babel-loader": "8.2.2",
		"babel-plugin-istanbul": "^6.0.0",
		"babel-plugin-lodash": "^3.3.4",
		"chai": "^4.3.4",
		"cross-env": "^7.0.3",
		"glob": "^7.1.6",
		"husky": "4.3.8",
		"npm-run-all": "^4.1.5",
		"npm-watch": "^0.11.0",
		"nyc": "^15.1.0",
		"postcss-url": "10.1.1",
		"prettier": "2.2.1",
		"pretty-quick": "3.1.0",
		"rimraf": "^3.0.2",
		"rollup": "2.39.0",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-filesize": "^9.1.0",
		"rollup-plugin-node-externals": "^2.2.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-postcss": "^4.0.0",
		"slash": "3.0.0"
	},
	"scripts": {
		"clean": "rimraf dist",
		"start": "run-s clean && run-p start:es start:cjs start:lib:es",
		"start:lib:es": "cross-env BABEL_ENV=es babel --watch src -d lib --verbose",
		"start:cjs": "cross-env NODE_ENV=cjs rollup -c -w",
		"start:es": "cross-env NODE_ENV=es rollup -c -w",
		"build": "run-s clean && run-p build:es build:cjs build:lib:es",
		"build:es": "cross-env NODE_ENV=es rollup -c",
		"build:cjs": "cross-env NODE_ENV=cjs rollup -c",
		"build:lib:es": "npm-run-all build:babel:lib:es",
		"build:babel:lib:es": "cross-env BABEL_ENV=es babel src -d lib",
		"format": "prettier --write ."
	}
}
