{
	"name": "@gisatcz/ptr-timeline",
	"version": "0.1.6",
	"description": "Panther timeline React component",
	"keywords": [
		"panther",
		"gisat",
		"timeline",
		"component"
	],
	"homepage": "https://github.com/gisat-panther/ptr-timeline",
	"prettier": "@gisatcz/prettier-config",
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"style": "dist/style.css",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"files": [
		"dist",
		"lib"
	],
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/gisat-panther/ptr-timeline"
	},
	"peerDependencies": {
		"react": "^16.13.1 || ^17.0.2"
	},
	"dependencies": {
		"@gisatcz/ptr-core": "^1.5.0",
		"@gisatcz/ptr-utils": "^1.5.0",
		"classnames": "^2.3.1",
		"eslint": "^8.8.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.28.0",
		"lodash": "^4.17.21",
		"moment": "2.29.1",
		"prop-types": "^15.7.2",
		"react-resize-detector": "^6.7.6"
	},
	"devDependencies": {
		"@babel/cli": "^7.12.16",
		"@babel/core": "^7.15.8",
		"@babel/eslint-parser": "^7.17.0",
		"@babel/plugin-transform-runtime": "^7.15.8",
		"@babel/preset-env": "^7.15.8",
		"@babel/preset-react": "^7.16.7",
		"@babel/register": "^7.12.13",
		"@gisatcz/prettier-config": "^0.0.3",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^21.0.0",
		"babel-plugin-istanbul": "^6.0.0",
		"babel-plugin-lodash": "^3.3.4",
		"chai": "^4.3.4",
		"cross-env": "^7.0.3",
		"glob": "^7.1.6",
		"husky": "^4.3.8",
		"npm-run-all": "^4.1.5",
		"path": "^0.12.7",
		"postcss": "^8.3.9",
		"postcss-url": "^10.1.3",
		"prettier": "^2.4.1",
		"pretty-quick": "^3.1.1",
		"react": "^17.0.2",
		"rimraf": "^3.0.2",
		"rollup": "^2.58.0",
		"rollup-plugin-filesize": "^9.1.0",
		"rollup-plugin-postcss": "^4.0.0",
		"sass": "^1.42.1"
	},
	"scripts": {
		"clean": "rimraf dist",
		"start": "run-s clean && run-p start:es start:cjs start:lib:es",
		"start:lib:es": "cross-env BABEL_ENV=es babel --watch src -d lib --verbose",
		"start:cjs": "cross-env NODE_ENV=cjs rollup -c -w",
		"start:es": "cross-env NODE_ENV=es rollup -c -w",
		"build": "run-s clean && run-p build:es build:cjs build:lib:es",
		"build:es": "cross-env NODE_ENV=es rollup -c",
		"build:cjs": "cross-env NODE_ENV=cjs rollup -c",
		"build:lib:es": "npm-run-all build:babel:lib:es",
		"build:babel:lib:es": "cross-env BABEL_ENV=es babel src -d lib",
		"format": "prettier --write .",
		"lint": "eslint .",
		"lintFix": "eslint . --fix"
	}
}
